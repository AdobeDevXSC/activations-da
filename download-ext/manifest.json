{
  "name": "Rename Express Downloads",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["downloads", "tabs"],
  "action": {"default_popup": "popup.html"},
  "background": { "service_worker": "bg.js" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cs.js"]
    }
  ]
}
